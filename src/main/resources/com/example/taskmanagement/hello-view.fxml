<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="494.0" prefWidth="665.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.taskmanagement.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <HBox fx:id="columna6" alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
      <children>
         <Button fx:id="btnWorkers" mnemonicParsing="false" onMouseClicked="#btnWorkerDisplay" text="Workers" />
         <Button fx:id="btnCleanses" mnemonicParsing="false" onMouseClicked="#btnCleansesDisplay" text="Cleanses" />
         <Button fx:id="btnTasks" onMouseClicked="#btnTasksDisplay" text="Tasks" />
      </children>
   </HBox>
   <TableView fx:id="listView" prefHeight="200.0" prefWidth="200.0">
     <columns>
       <TableColumn fx:id="columna1" prefWidth="75.0" text="C1" />
       <TableColumn fx:id="columna2" prefWidth="75.0" text="C2" />
         <TableColumn fx:id="columna3" prefWidth="75.0" text="C3" />
         <TableColumn fx:id="columna4" prefWidth="75.0" text="C4" />
         <TableColumn fx:id="columna5" prefWidth="75.0" text="C5" />
         <TableColumn fx:id="columna6" prefWidth="75.0" text="C6" />
         <TableColumn fx:id="columna7" prefWidth="75.0" text="C7" />
     </columns>
   </TableView>
</VBox>
